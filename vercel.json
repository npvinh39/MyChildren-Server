{
    "name": "my-children-server",
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "crons": [
        {
            "path": "/middleware/schedulePromotionVercel",
            "schedule": "*/10 * * * *"
        },
        {
            "path": "/middleware/scheduleRevenueVercel",
            "schedule": "0 0 * * *"
        }
    ]
}